<mujoco>
    <!-- <size njmax="3000" nconmax="1000" /> -->
    <!-- <compiler angle="degree" coordinate="local" inertiafromgeom="true" />
    <option timestep="0.004" />
    <statistic meansize="0.23" extent="3.96" /> -->
    <default>
        <geom contype="0" conaffinity="0" condim="6" density="5" size=".05" type="sphere" />
        <joint damping=".001" />
        <motor ctrlrange="-1 1" ctrllimited="true" forcerange="-.02 .02" forcelimited="true" />
        <site size="0.032" type="sphere" />
    </default>
    <worldbody>
        <!-- <geom name="floor" size="5 5 0.1" type="plane" condim="6"/> -->
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1"
            pos="0 0 1.3" specular=".1 .1 .1" />
        <body name="car" pos="0 0 .1">
            <site name="car" pos="0 0 0" />
            <camera name="vision" pos="0 0 .2" xyaxes="-1 0 0 0 -.4 1" fovy="90" />
            <camera name="vision_back" pos="0 0 .2" xyaxes="1 0 0 0 0 1" fovy="90" />
            <!-- <joint type="free" name="car" damping="0" /> -->
            <geom name="car" type="box" size=".1 .1 .05" rgba="0.7412 0.0431 0.1843 1" />
            <geom name="back_bumper" type="box" pos="0 .15 0" size=".1 .01 .05"
                rgba="0.0039 0.1529 0.3961 1" />
            <geom name="back_connector" type="box" pos="0 .125 0" size=".01 .025 .03"
                rgba="0.0039 0.1529 0.3961 1" />
            <geom name="front_bumper" type="box" pos="0 -.165 0" size=".05 .01 .05"
                rgba="0.0039 0.1529 0.3961 1" />
            <geom name="front_connector" type="box" pos="0 -.13 .04" size=".05 .03 .01"
                rgba="0.0039 0.1529 0.3961 1" />
            <body name="left" pos="-.1 .1 -.05">
                <joint type="hinge" name="left" axis="1 0 0" />
                <geom type="cylinder" name="left" fromto="-.055 0 0 -0.005 0 0"
                    rgba="0.7412 0.0431 0.1843 1" />
            </body>
            <body name="right" pos=".1 .1 -.05">
                <joint type="hinge" name="right" axis="1 0 0" />
                <geom type="cylinder" name="right" fromto="0.005 0 0 .055 0 0"
                    rgba="0.7412 0.0431 0.1843 1" />
            </body>
            <body name="rear" pos="0 -.1 -.05">
                <joint name="rear" type="ball" />
                <geom name="rear" rgba="0.7412 0.0431 0.1843 1" />
            </body>
        </body>
    </worldbody>
    <sensor>
        <!--<jointvel
        joint="left" name="jointvel_left"/>
        <jointvel joint="right" name="jointvel_right"/>-->

        <ballquat joint="rear" name="ballquat_rear" />
        <ballangvel joint="rear" name="ballangvel_rear" />

        <!-- Used for observation -->
        <accelerometer site="car" name="accelerometer" />
        <velocimeter site="car" name="velocimeter" />
        <gyro site="car" name="gyro" />
        <magnetometer site="car" name="magnetometer" />

        <!-- Used for intrinsic constraints -->
        <subtreecom body="car" name="subtreecom" />
        <subtreelinvel body="car" name="subtreelinvel" />
        <subtreeangmom body="car" name="subtreeangmom" />
    </sensor>
    <actuator>
        <motor gear="1" jointinparent="left" name="left" />
        <motor gear="1" jointinparent="right" name="right" />
    </actuator>
</mujoco>