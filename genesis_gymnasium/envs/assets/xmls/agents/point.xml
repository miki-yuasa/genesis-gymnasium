<mujoco>
    <!-- <size njmax="3000" nconmax="1000" />
    <option timestep="0.002" />
    <statistic meansize="0.23" extent="3.96" /> -->
    <default>
        <geom contype="0" conaffinity="0" condim="6" density="1" />
        <joint damping=".001" />
        <motor ctrlrange="-1 1" ctrllimited="true" forcerange="-.05 .05" forcelimited="true" />
        <velocity ctrlrange="-1 1" ctrllimited="true" forcerange="-.05 .05" forcelimited="true" />
        <site size="0.032" type="sphere" />
    </default>
    <worldbody>
        <light cutoff="100" diffuse="1 1 1" dir="-0 0 -1.3" directional="true" exponent="1"
            pos="0 0 1.3" specular=".1 .1 .1" />
        <body name="point" pos="0 0 .1">
            <camera name="vision" pos="0 0 .15" xyaxes="0 -1 0 .4 0 1" fovy="90" />
            <camera name="vision_back" pos="0 0 .15" xyaxes="0 1 0 0 0 1" fovy="90" />
            <joint type="slide" axis="1 0 0" name="x" damping="0.01" />
            <joint type="slide" axis="0 1 0" name="y" damping="0.01" />
            <joint type="hinge" axis="0 0 1" name="z" damping="0.005" />
            <geom name="point" type="sphere" size="0.1" friction="1 0.01 0.01"
                rgba="0.7412 0.0431 0.1843 1" />
            <geom name="pointarrow" pos="0.1 0 0" size="0.05 0.05 0.05" type="box"
                rgba="0.0039 0.1529 0.3961 1" />
            <site name="point" rgba="1 0 0 .1" />
        </body>
    </worldbody>
    <sensor>
        <!-- Used for observation -->
        <accelerometer site="point" name="accelerometer" />
        <velocimeter site="point" name="velocimeter" />
        <gyro site="point" name="gyro" />
        <magnetometer site="point" name="magnetometer" />

        <!-- Used for intrinsic constraints -->
        <subtreecom body="point" name="subtreecom" />
        <subtreelinvel body="point" name="subtreelinvel" />
        <subtreeangmom body="point" name="subtreeangmom" />
    </sensor>
    <actuator>
        <motor gear="0.3 0 0 0 0 0" site="point" name="x" />
        <velocity gear="0.3" jointinparent="z" name="z" />
    </actuator>
</mujoco>